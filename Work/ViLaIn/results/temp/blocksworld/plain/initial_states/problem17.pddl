(:objects
    yellow_block - block
    pink_block - block
    green_block - block
    red_block - block
    purple_block - block
    blue_block - block
    robot - robot
)
(:init 
    (ontable yellow_block)
    (clear pink_block)
    (on green_block red_block)
    (on red_block yellow_block)
    (on yellow_block purple_block)
    (on purple_block blue_block)
    (handempty robot)
)